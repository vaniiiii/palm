{
  "name": "@palm/circuits",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:echo": "mkdir -p build && circom circuits/palm-echo.circom -l ./node_modules -l ../../node_modules --wasm --r1cs --sym -o ./build",
    "build:legion": "mkdir -p build && circom circuits/palm-legion.circom -l ./node_modules -l ../../node_modules --wasm --r1cs --sym -o ./build",
    "build": "bun run build:echo && bun run build:legion",
    "test": "NODE_OPTIONS=--max_old_space_size=16384 jest",
    "witness:echo": "bun run scripts/test-witness.ts echo",
    "witness:legion": "bun run scripts/test-witness.ts legion",
    "setup": "NODE_OPTIONS=--max_old_space_size=16384 npx tsx scripts/dev-setup.ts",
    "setup:echo": "NODE_OPTIONS=--max_old_space_size=16384 npx tsx scripts/dev-setup.ts echo",
    "setup:legion": "NODE_OPTIONS=--max_old_space_size=16384 npx tsx scripts/dev-setup.ts legion",
    "prove": "NODE_OPTIONS=--max_old_space_size=16384 npx tsx scripts/generate-proof.ts"
  },
  "dependencies": {
    "@zk-email/circuits": "^6.1.3",
    "@zk-email/helpers": "^6.1.3",
    "@zk-email/zk-regex-circom": "^2.3.1",
    "circomlib": "^2.0.5",
    "commander": "^12.0.0",
    "snarkjs": "^0.7.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^22.0.0",
    "babel-preset-jest": "^29.6.3",
    "circom_tester": "^0.0.19",
    "circomlibjs": "^0.1.7",
    "jest": "^29.5.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0"
  }
}
